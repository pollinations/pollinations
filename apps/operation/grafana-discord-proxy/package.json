{
    "name": "grafana-discord-proxy",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "decrypt": "sops -d --output-type dotenv secrets/env.json > .dev.vars",
        "dev": "npm run decrypt && wrangler dev",
        "deploy": "wrangler deploy",
        "secret:set": "wrangler secret put DISCORD_WEBHOOK_URL"
    },
    "devDependencies": {
        "wrangler": "^3.0.0"
    }
}